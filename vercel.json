{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/real-time-bus-stop-density-dashboard"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\..*)",
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
